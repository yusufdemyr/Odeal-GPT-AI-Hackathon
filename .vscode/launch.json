    "version": "0.2.0",
    "configurations": [
        
        // run config for npm run dev in frontend
        {
            "name": "npm: run dev",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}/Chatbot/frontend",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script",
                "dev"
            ],
        },
        {
            "name": "Python: FastAPI",
            "type": "python",
            "python": "${workspaceFolder}/Chatbot/api/env/Scripts/python.exe",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main:app",
                "--reload"
            ],
            "jinja": true,
            "justMyCode": true,
            "cwd": "${workspaceFolder}/Chatbot/api",
        }
    ]
}